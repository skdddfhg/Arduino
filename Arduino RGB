int RED = 11; 
int GRE = 10;
int BLU = 9;

void setup(){
  pinMode(RED, OUTPUT);
  pinMode(GRE, OUTPUT);
  pinMode(BLU, OUTPUT);
  Serial.begin(9600);
}

void loop(){
    char command;
    for(int i = 0; i <= 255 ; i += 5){
      analogWrite(RED, i);
      delay(50);
      Serial.println(i);
    }
    for(int i = 255 ; i >= 0 ; i -= 5){
      analogWrite(RED, i);
      delay(50);
      Serial.println(i);
    }
}
